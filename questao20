public class questao20 {

	public static void main(String[] args) {
		carro c1= new carro();
		c1.ignicao= false;
		c1.vel_inicial= 10;
		
		c1.partida();
		
		System.out.println("\n");
		
		carro c2= new carro();
		c2.ignicao= true;
		c2.vel_inicial= 25;
		
		c2.vel(15);
		c2.frear(5);
	}

}
