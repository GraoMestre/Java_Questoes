public class retangulo {
	double largura,perimetroA;
	double altura, areaA;
	
	void area() {
		areaA= largura*altura;
		System.out.println("√Årea= " + areaA);
	}
	
	void perimetro(){
		perimetroA= (2*largura) + (2*altura);
		System.out.println("Perimetro= " + perimetroA);
	}
	
}

